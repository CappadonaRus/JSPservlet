<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>web.webServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>jQueryServlet</servlet-name>
        <servlet-class>web.jQueryFirstTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jQueryServlet</servlet-name>
        <url-pattern>/data</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>jQuerySecondTable</servlet-name>
        <servlet-class>web.jQuerySecondTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jQuerySecondTable</servlet-name>
        <url-pattern>/data2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>jQueryThirdTable</servlet-name>
        <servlet-class>web.jQueryThirdTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jQueryThirdTable</servlet-name>
        <url-pattern>/data3</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FinalDataServlet</servlet-name>
        <servlet-class>web.FinalDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FinalDataServlet</servlet-name>
        <url-pattern>/fin</url-pattern>
    </servlet-mapping>

</web-app>